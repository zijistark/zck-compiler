QUEX_NAMESPACE_TOKEN_OPEN

#define __QUEX_FROM      cp1252

#include <quex/code_base/lexeme_converter/generator/declarations.g>

QUEX_NAMESPACE_TOKEN_CLOSE

#endif /* __QUEX_INCLUDE_GUARD__LEXEME_CONVERTER__cp1252__                */
